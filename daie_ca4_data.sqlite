-- tables
-- Table: Asset
CREATE TABLE Asset (
    id int NOT NULL,
    type varchar(32) NOT NULL,
    format varchar(32) NOT NULL,
    created_by varchar(32) NOT NULL,
    Library_id int NOT NULL,
    CONSTRAINT Asset_pk PRIMARY KEY (id)
);

-- Table: Library
CREATE TABLE Library (
    id int NOT NULL,
    Asset varchar(32) NOT NULL,
    Project_id int NOT NULL,
    CONSTRAINT Library_pk PRIMARY KEY (id)
);

-- Table: Project
CREATE TABLE Project (
    id int NOT NULL,
    name varchar(32) NOT NULL,
    members varchar(32) NOT NULL,
    output varchar(32) NOT NULL,
    delivery_date date NOT NULL,
    CONSTRAINT Project_pk PRIMARY KEY (id)
);

-- Table: Team_Member
CREATE TABLE Team_Member (
    id int NOT NULL,
    Job_Type varchar(32) NOT NULL,
    CONSTRAINT Team_Member_pk PRIMARY KEY (id)
);

-- Table: Work_Item
CREATE TABLE Work_Item (
    id int NOT NULL,
    name varchar(32) NOT NULL,
    status varchar(32) NOT NULL,
    assigned_to varchar(32) NOT NULL,
    Project_id int NOT NULL,
    Team_Member_id int NOT NULL,
    CONSTRAINT Work_Item_pk PRIMARY KEY (id)
);
